			},
			{
				"pagePath": "pages/message/index",
				"iconPath": "static/images/tabbar/message-dark.png",
				"selectedIconPath": "static/images/tabbar/message-light.png",
				"text": "消息"
			},
			{
				"pagePath": "pages/user/index",
				"iconPath": "static/images/tabbar/user-dark.png",
